{
    "manifest_version": 2,
    "name": "Quote Unquote",
    "description": "Share websites and words with just a click.",
    "version": "2",
    "author": "Vidhi Bagadia",
    

    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "Quote.png"
    },

    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*"
    ]
}